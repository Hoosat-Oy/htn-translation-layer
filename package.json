{
  "type": "module",
  "name": "hoosat",
  "version": "1.0.0",
  "description": "Hoosat Website",
  "author": "Toni Lukkaroinen",
  "license": "Proprietary",
  "main": "index.js",
  "bugs": {
    "url": "https://github.com/Hoosat-Oy/hoosat-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Hoosat-Oy/hoosat-template"
  },
  "scripts": {
    "test": "jest",
    "clean:build-dev": "rm -rf build-dev",
    "clean:build-ts": "rm -rf build-ts",
    "clean:build": "rm -rf build",
    "clean": "npm-run-all -p clean:build-ts clean:build-dev clean:build",
    "dev:compress:assets:br": "node --loader ts-node/esm build-tools/compress-files.ts build-dev/public/ br",
    "dev:compress:assets:deflate": "node --loader ts-node/esm build-tools/compress-files.ts build-dev/public/ deflate",
    "dev:compress:assets:gzip": "node --loader ts-node/esm build-tools/compress-files.ts build-dev/public/ gzip",
    "dev:compress:assets": "npm-run-all dev:compress:assets:br dev:compress:assets:deflate dev:compress:assets:gzip",
    "dev:create:build:directories": "npx mkdirp build-dev && npx mkdirp build-dev/public && npx mkdirp build-dev/public/uploads",
    "dev:create:service-worker:urls": "node public/service-worker-urls.js build-dev/public",
    "dev:move:assets": "npx ncp public build-dev/public",
    "dev:build:server": "webpack --config webpack.server.config.js --mode=development",
    "dev:prebuild": "npm-run-all dev:create:build:directories dev:move:assets dev:create:service-worker:urls",
    "dev:build": "npm-run-all dev:prebuild dev:build:server dev:compress:assets",
    "dev:start": "nodemon --experimental-modules build-dev/server.js development",
    "dev:watch": "chokidar './src/' './public/' -c 'npm-run-all dev:build'",
    "dev": "npm-run-all clean:build-ts clean:build-dev dev:build -p dev:watch dev:start",
    "compress:assets:br": "node --loader ts-node/esm build-tools/compress-files.ts build/public/ br",
    "compress:assets:deflate": "node --loader ts-node/esm build-tools/compress-files.ts build/public/ deflate",
    "compress:assets:gzip": "node --loader ts-node/esm build-tools/compress-files.ts build/public/ gzip",
    "compress:assets": "npm-run-all compress:assets:br compress:assets:deflate compress:assets:gzip",
    "create:build:directories": "npx mkdirp build && npx mkdirp build/public && npx mkdirp build/public/uploads",
    "create:service-worker:urls": "node public/service-worker-urls.js build/public",
    "move:assets": "npx ncp public build/public",
    "build:server": "webpack --config webpack.server.config.js --mode=production",
    "prebuild": "npm-run-all clean:build-ts clean:build create:build:directories move:assets create:service-worker:urls ",
    "build": "npm-run-all prebuild build:server compress:assets",
    "deploy": "npm-run-all build rsync clean:build clean:build-ts",
    "production:start": "node --experimental-modules build/server.js",
    "start": "npm run production:start",
    "rsync": "rsync -rPz --delete --exclude='./build/public/uploads'--exclude='./build/public/hoobot' ./build ./package.json tonto@hoosat.fi:~/address-layer.hoosat.fi"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "formidable": "^3.5.1",
    "google-auth-library": "^9.9.0",
    "i18next-fs-backend": "^2.3.1",
    "mongoose": "^8.3.2",
    "nodemailer": "^6.9.13",
    "react-helmet-async": "^2.0.4",
    "react-i18next": "^14.1.1",
    "react-router-dom": "^6.23.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.1.2",
    "@types/formidable": "^3.4.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^16.11.10",
    "@types/nodemailer": "^6.4.14",
    "@types/react-scroll": "^1.8.10",
    "@types/react-syntax-highlighter": "^15.5.10",
    "@types/rss": "^0.0.32",
    "@types/sync-fetch": "^0.4.3",
    "@types/uuid": "^9.0.2",
    "@types/webpack": "^5.28.1",
    "@types/ws": "^8.5.5",
    "chokidar-cli": "^3.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.7.3",
    "esm": "^3.2.25",
    "file-loader": "^6.2.0",
    "ignore-loader": "^0.1.2",
    "jest": "^29.5.0",
    "json-loader": "^0.5.7",
    "mkdirp": "^3.0.1",
    "ncp": "^2.0.0",
    "node-loader": "^2.0.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "style-loader": "^3.3.2",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "keywords": []
}
